    "SheetName": "NpcYell",
    "SheetLanguage": 2
  },
  {
    "Text": "That he should place such faith in Golbez...",
    "BalloonTime": 5,
    "Unknown0": 0,
    "OutputType": 3,
    "Unknown1": 1,
    "Unknown2": 25,
    "Unknown3": false,
    "Unknown4": false,
