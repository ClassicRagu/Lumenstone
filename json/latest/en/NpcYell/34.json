    "SheetName": "NpcYell",
    "SheetLanguage": 2
  },
  {
    "Text": "I\u0027m sure this was a beautiful castle once...",
    "BalloonTime": 5,
    "Unknown0": 0,
    "OutputType": 3,
    "Unknown1": 1,
    "Unknown2": 25,
    "Unknown3": false,
    "Unknown4": false,
