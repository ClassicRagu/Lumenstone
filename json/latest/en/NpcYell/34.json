    "RowId": 16509,
    "SubRowId": 0,
    "SheetName": "NpcYell",
    "SheetLanguage": 2
  },
  {
    "Text": "Let us leave the other side to him.",
    "BalloonTime": 5,
    "Unknown0": 0,
    "OutputType": 3,
    "Unknown1": 1,
    "Unknown2": 25,
    "Unknown3": false,
    "Unknown4": false,
