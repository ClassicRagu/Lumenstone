    "RowId": 16504,
    "SubRowId": 0,
    "SheetName": "NpcYell",
    "SheetLanguage": 2
  },
  {
    "Text": "Who could be behind all this?",
    "BalloonTime": 5,
    "Unknown0": 0,
    "OutputType": 3,
    "Unknown1": 1,
    "Unknown2": 25,
    "Unknown3": false,
    "Unknown4": false,
